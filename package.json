{
  "name": "ctrip-apollo-client-ts",
  "version": "1.1.3",
  "description": "Ctrip Apollo config client.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "ctrip-apollo-client",
    "apollo-config",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:wangliguo6666/ctrip-apollo-client-ts.git"
  },
  "author": "wangliguo wangliguo0149@126.com",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public --registry=https://registry.npmjs.org",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --registry=https://registry.npmjs.org --tag next"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "get-value": "^3.0.1",
    "ip": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@types/get-value": "^3.0.1",
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
